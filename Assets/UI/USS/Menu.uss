.menuButton:hover {
    background-image: url('project://database/Assets/Arts/Sprites/blue_button02.png?fileID=2800000&guid=18a145bd830caca459b252ce58a73cfd&type=3#blue_button02');
}

.menuButton:focus {
    background-image: url('project://database/Assets/Arts/Sprites/blue_button01.png?fileID=2800000&guid=75c5e799e06c7b34d803ce7d939972bc&type=3#blue_button01');
