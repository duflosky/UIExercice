}

.window {
    background-color: rgba(100, 100, 100, 0.78);
    border-top-left-radius: 20px;
    border-top-right-radius: 20px;
    border-bottom-right-radius: 20px;
    border-bottom-left-radius: 20px;
    max-height: 75%;
    min-height: 75%;
    max-width: 50%;
    min-width: 50%;
    left: 25%;
    top: 12.5%;
    -unity-font: url('project://database/Assets/Arts/Fonts/kenvector_future.ttf?fileID=12800000&guid=c7c77b82c35048a408906d60ad82c228&type=3#kenvector_future');
    -unity-font-definition: url('project://database/Assets/Arts/Fonts/kenvector_future%20SDF.asset?fileID=11400000&guid=03e56dd485b551a478059d390994354d&type=2#kenvector_future SDF');
}

.title {
    -unity-font: url('project://database/Assets/Arts/Fonts/kenvector_future.ttf?fileID=12800000&guid=c7c77b82c35048a408906d60ad82c228&type=3#kenvector_future');
    font-size: 64px;
    -unity-font-style: bold;
    -unity-text-align: middle-center;
    margin-top: 5px;
    margin-left: 5px;
    -unity-font-definition: url('project://database/Assets/Arts/Fonts/kenvector_future%20SDF.asset?fileID=11400000&guid=03e56dd485b551a478059d390994354d&type=2#kenvector_future SDF');
}

.options {
    -unity-font-style: bold;
